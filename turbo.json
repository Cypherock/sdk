{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "wasm/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start:node": {
      "dependsOn": ["^start:node"],
      "cache": false
    },
    "dev:browser": {
      "dependsOn": ["^dev:browser"],
      "cache": false
    },
    "start:browser": {
      "dependsOn": ["^start:browser"],
      "cache": false
    }
  }
}
