{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prebuild": {
      "cache": true,
      "inputs": ["scripts/prebuild.sh"]
    },
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": ["dist/**", ".next/**", "wasm/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    },
    "lint:check": {
      "inputs": ["."],
      "outputs": []
    },
    "lint": {
      "inputs": ["."],
      "outputs": []
    },
    "pretty:check": {
      "inputs": ["."],
      "outputs": []
    },
    "pretty": {
      "inputs": ["."],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "@cypherock/sdk-core#prebuild": {
      "cache": true,
      "outputs": ["src/encoders/proto/**"],
      "inputs": ["../../submodules/common/proto/**"]
    },
    "@cypherock/sdk-app-manager#prebuild": {
      "cache": true,
      "outputs": ["src/proto/**"],
      "inputs": ["../../submodules/common/proto/**"]
    },
    "start:node": {
      "dependsOn": ["^start:node"],
      "cache": false
    },
    "dev:browser": {
      "dependsOn": ["^dev:browser"],
      "cache": false
    },
    "start:browser": {
      "dependsOn": ["^start:browser"],
      "cache": false
    },
    "pre-commit": {
      "inputs": ["."],
      "cache": true
    }
  }
}
