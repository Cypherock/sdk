{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "wasm/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start:node": {
      "dependsOn": ["^build"]
    },
    "nodejs-test#start:node": {
      "cache": false
    },
    "dev:browser": {
      "dependsOn": ["^dev:browser"],
      "cache": false
    },
    "browser-test#dev:browser": {
      "dependsOn": ["^build"]
    },
    "start:browser": {
      "dependsOn": ["^start:browser"]
    },
    "browser-test#start:browser": {
      "dependsOn": ["^build", "browser-test#build"]
    }
  }
}
