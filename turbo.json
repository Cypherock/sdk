{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start:node": {
      "dependsOn": ["build", "^start:node"]
    },
    "dev:browser": {
      "dependsOn": ["build", "^dev:browser"],
      "cache": false
    },
    "start:browser": {
      "dependsOn": ["build", "^start:browser"]
    }
  }
}
