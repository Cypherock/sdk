    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.4.1",
    "mocha": "^10.2.0",
    "prettier-config-custom": "workspace:^0.0.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-proto": "^1.139.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@cypherock/sdk-interfaces": "workspace:^0.0.0",
    "compare-versions": "6.0.0-rc.1",
